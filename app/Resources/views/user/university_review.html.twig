{% extends 'base.html.twig' %}

{% block body %}
    <h1>Students list</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>User Name</th>
                <th>Email</th>
                <th colspan="3">Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td><a href="{{ path('user_show', { 'id': user.id }) }}">{{ user.id }}</a></td>
                <td>{{ user.username }}</td>
                <td>{{ user.email }}</td>
                <td><a href="{{ path('university_changed_user_review', { 'id': user.id }) }}">Review</a></td>

              
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
